<!DOCTYPE html>
<html>
<head>
  <title>My-journal Administrator</title>
  <%= stylesheet_link_tag "style-admin-two", :media => "all" %>
  <%= stylesheet_link_tag "bootstrap-two", :media => "all" %>
  <%= stylesheet_link_tag "main-style", :media => "all" %>
  <%= stylesheet_link_tag "font-awesome", :media => "all" %>
  <%= stylesheet_link_tag "pace-theme-big-counter", :media => "all" %>
  <%= stylesheet_link_tag "scroller", :media => "all" %>
  <%= stylesheet_link_tag "modal", :media => "all" %>
  <%= javascript_include_tag "jquery-1.9.1.min" %>
  <%= javascript_include_tag "application-admin" %>
  <%#= javascript_include_tag "numeric" %>
  <%= csrf_meta_tags %>
</head>

<body>
<!--  wrapper -->
      <div id="wrapper">
             <%= render :partial => "admin/shared/header_admin" %>
             <%= render :partial => "admin/shared/navbar" %>
             <!--  page-wrapper -->
                    <div id="page-wrapper">
                          <%= render :partial => "admin/shared/page" %>
                          <%= render :partial => "admin/shared/notice" %>
                          <div class="row">
                                <%= yield %>
                                <!-- Chat Panel Example-->
                                  <%= render partial: 'admin/messages/chat' %>
                                <!--End Chat Panel Example-->
                          </div>
                    </div>
             <!-- end page-wrapper -->
      </div>
<!-- end wrapper -->
</body>
</html>
