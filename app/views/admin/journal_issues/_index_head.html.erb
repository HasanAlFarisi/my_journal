<%= javascript_include_tag "admin/journal" %>
<%= javascript_include_tag "admin/tooltip" %>
<div class="panel panel-primary">
      <div class="panel-heading">
          <i class="fa fa-bar-chart-o fa-fw"></i><spans class="assign"><%= @admin_journal.title %> Journal(s)</spans>
      </div>

      <div class="panel-body">
          <div class="row">
              <div class="col-lg-12">
                  <div class="table-responsive">
                      <table class="table table-bordered table-hover table-striped">
                          <thead>
                              <tr>
                                  <th>Number</th>
                                  <th width="120px">Project</th>
                                  <th width="120px">Status</th>
                                  <th>Priority</th>
                                  <th>Progress</th>
                                  <th width="380px">Title</th>
                                  <th colspan="2"><center>Action</center></th>
                              </tr>
                          </thead>
                              <tbody class="get_index">
                                    <tr class="ajaxs hide">
                                            <td colspan="8"><center><%= render 'dashboard/shared/ajax_loader' %></center></td>
                                    </tr>
                                    <%= render "admin/journals/get_index" %>
                              </tbody>
                      </table>
                      <div class="left">Show: <b class="shows"><%= @admin_journals_status_filter.blank? ? 0 : @admin_journals_status_filter.count %></b> data From: <b class="froms"><%= @admin_journals_status_filter.blank? ? 0 : @admin_journals_status_filter.count %></b> data
                        <span class="pull-right">
                              <span class="paginate">
                                <%#= will_paginate @admin_journals %>
                                &nbsp;
                              </span>
                              <% if @admin_journals.blank? %>
                                    <a role="button" id="new_journal" href="/admin/journal_issues/create_project" class="btn btn-primary"><span class="fa  fa-pencil-square fa-1x"></span>New Journal</a>
                              <% else %>
                                    <a role="button" onclick="new_journals(<%= session[:session_admin] %>)" id="new_journal" href="#new_journal" data-url="new_journal" data-toggle="modal" class="btn btn-primary"><span class="fa  fa-pencil-square fa-1x"></span>New Journal</a>
                              <% end %>
                        </span>
                      </div>
                  </div>

              </div>

          </div>
          <!-- /.row -->
      </div>
      <!-- /.panel-body -->
  </div>