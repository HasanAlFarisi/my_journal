<%= javascript_include_tag "admin/journal" %>
<div id="modal_background"></div>
<div id="task-image-popups"></div>

<div class="panel panel-<%= @estimate == 'Out of Deadline' ? 'danger' : 'primary' %>">
      <div class="panel-heading">
          <i class="fa fa-bookmark fa-fw"></i><%= @admin_journal.title %>
             <div class="pull-right">
                  <%= @estimate.html_safe %>
             </div>
      </div>

      <div class="panel-body">
          <div class="row">
              <div class="col-lg-12">
                    <span class="pull-right">
                          <%= raw generated_link_to("<i class='fa  fa-pencil fa-1x'></i> Edit Status Task".html_safe,"#update_journal",{:class => 'btn btn-info',:id=>"update_journals",:role=>"button", :onclick => "update_journal(#{@admin_journal_issue.id})", :data => {:toggle=>"modal", :url=>"update_journals"}, style:"padding:9px"})%>
                    </span>
                    <strong class="journal_title"><%= "##{@number} " "#{@admin_journal_issue.title}" %></strong>
                    <span class="clearfix"></span>
                    Starting <%= link_to @admin_journal_issue.start.strftime('%d-%b-%Y'), "javascript:void(0)" %> Ending <%= link_to @admin_journal_issue.end.strftime('%d-%b-%Y'), "javascript:void(0)" %>
                    <span class="clearfix"></span>
                    <%= cl_image_tag "profiles/#{@admin_journal.admin_id}",class:"img-icon floating" %> 
                    <nav>
                      &nbsp;Assign to :
                    <b>
                          <%= raw generated_link_to("#{find_profile(@admin_journal_issue.asignee).name} #{find_profile(@admin_journal_issue.asignee).last_name}","#view_profile",{:id=>"view_profiles",:role=>"button", :onclick => "view_profile(#{@admin_journal_issue.asignee},'#{current_admin.id == @admin_journal_issue.asignee ? 'no' : 'yes'}')", :data => {:toggle=>"modal", :url=>"view_profiles"}})%>
                    </b>
                    From : 
                    <b>
                          <%= raw generated_link_to("#{find_profile(@admin_journal_issue.from).name} #{find_profile(@admin_journal_issue.from).last_name}","#view_profile",{:id=>"view_profiles",:role=>"button", :onclick => "view_profile(#{@admin_journal_issue.from},'#{current_admin.id == @admin_journal_issue.from ? 'no' : 'yes'}')", :data => {:toggle=>"modal", :url=>"view_profiles"}})%>
                    </b>
                    </nav>
                    <nav>
                      &nbsp;Project Manajer : <strong><%= @admin_journal.project_manajer %></strong>
                    </nav>
                    <nav>
                      &nbsp;Project Quality Checks:
                      <% @admin_journal_issue.journal_issue_asignees.each do |journal_team| %>
                              <strong><%= "#{journal_team.admin_id.blank? ? "" : find_name(journal_team.admin_id)} " %></strong>
                      <% end %> 
                    </nav>
                    <p style="text-align:center">
                          <br>
                          <b class="add-padding-right">
                                <%= link_to find_status(@admin_journal_issue.status_id).html_safe, admin_journal_issue_list_data_by_status_path(@admin_journal_issue.status_id) %>
                                <% if @admin_journal_issue.status_id == 6 %>
                                <%=  @admin_journal_issue.progress %>%
                                <% end %>
                          </b>
                          <% Admin::StatusJournal.where('id != ?',@admin_journal_issue.status_id).each do |status| %>
                          <span class="add-padding-right">[ <%= link_to status.name, admin_journal_issue_list_data_by_status_path(status.id) %> ]</span>
                          <% end %>
                    </p>
                    <div class="contents">
                          <%= raw @admin_journal_issue.content %>
                    </div>
                    <% unless @admin_journal_issue.journal_issue_images.blank? %>
                          <br>
                          <div class="contents-none" style="text-align:left">
                                <b>Images</b>
                                <span class="clearfix"></span>
                                <% @admin_journal_issue.journal_issue_images.each do |image| %>
                                       <%= raw generated_link_to(cl_image_tag("journal_issue/images/#{image.id}", class:"img_profile_issue", style:"position: relative;width:25%;height:25%"),"#view_image",{:id=>"view_image",:role=>"button", :onclick => "view_image(#{image.id})", :data => {:toggle=>"modal", :url=>"view_images"}})%>
                                <% end %>
                          </div>
                    <% end %>
                    <% unless @admin_journal_issue.journal_issue_files.blank? %>
                          <br>
                          <div class="contents-none" style="text-align:left">
                                <b>Files</b>
                                <span class="clearfix"></span>
                                <% @admin_journal_issue.journal_issue_files.each do |file| %>
                                       <%= link_to generated_file_type(file).html_safe, cloudinary_url("http://res.cloudinary.com/sampulkreativ/raw/upload/v1/journal_issue/files/#{file.id}_#{file.document_file_name}", :resource_type => :raw), :target => "_blank" %>
                                <% end %>
                          </div>
                    <% end %>
                    <span class="clearfix"></span>
                    <br>
                    <div class="contents-none" style="margin-bottom:-30px; <%= 'display:none' if @admin_journal_issue.journal_issue_comments.blank? %>">
                          <%= render "admin/journal_issue_comment/all_comment" %>
                          <%= render 'dashboard/shared/ajax_loader' %>
                          <div class="temp-comment">
                                <!-- Place for Comment with render as JS -->       
                          </div>
                   </div>
                   <div class="contents-none">
                         <%= link_to "Back", admin_journals_path, class:"btn btn-default  pull-right" %>
                         <% if @admin_journal.admin_id == current_admin.id %>
                                <%= link_to "<i class='fa  fa-pencil fa-1x'></i>Edit".html_safe, edit_admin_journal_issue_path(@admin_journal_issue, id_journal: @admin_journal_issue.journal_id),class:"btn btn-success", id:"button-create", style:"padding:9px" %>
                         <% end %>
                  </div>
              </div>
          </div>
      </div>
</div>
<%= render "admin/journal_issues/comment_task" %>