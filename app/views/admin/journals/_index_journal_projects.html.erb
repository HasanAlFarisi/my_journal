<div class="panel panel-primary">
      <div class="panel-heading">
          <i class="fa fa-bar-chart-o fa-fw"></i><%= find_profile(session[:session_admin]).name %> Project(s)
      </div>

      <div class="panel-body">
          <div class="row">
              <div class="col-lg-12">
                  <div class="table-responsive">
                      <table class="table table-bordered table-hover table-striped">
                          <thead>
                              <tr>
                                  <th width="13">No</th>
                                  <th>Title</th>
                                  <th>Status</th>
                                  <th>Assign</th>
                                  <th><center>Task</center></th>
                                  <th><center>Estimated Time</center></th>
                                  <th colspan="2" class="ac"><center>Action</center></th>
                              </tr>
                          </thead>
                          <tbody>
                              <% unless @admin_journals.present? %>
                                <tr class="odd">
                                  <td colspan="7">
                                    <center>No data yet</center>
                                  </td>
                                </tr>
                              <% end %>

                              <% @admin_journals.each_with_index do |admin_journal,n| %>
                                    <%= render partial: "admin/journals/list_journals", locals: {admin_journal: admin_journal, n: n} %>          
                              <% end %>
                          </tbody>
                      </table>
                      <div class="left">
                            <span class="floating">Total: <b><%= @admin_journals_status.count %></b> Project(s)</span>
                            <span class="pull-right paginate">                     
                                <%= will_paginate @admin_journals %>
                            </span>
                      </div>
                 </div>
             </div>
         </div>
      </div>
</div>