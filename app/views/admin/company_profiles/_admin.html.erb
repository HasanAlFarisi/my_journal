<div class="panel panel-primary">
      <div class="panel-heading">
          <i class="fa fa-user fa-fw"></i>SampulKreativ Members
          <div class="pull-right">
              <div class="btn-group">
                  <button type="button" class="btn btn-default btn-xs dropdown-toggle" data-toggle="dropdown">
                      Actions
                      <span class="caret"></span>
                  </button>
                  <ul class="dropdown-menu pull-right" role="menu">
                      <li><a href="#">Action</a>
                      </li>
                      <li><a href="#">Another action</a>
                      </li>
                      <li><a href="#">Something else here</a>
                      </li>
                      <li class="divider"></li>
                      <li><a href="#">Separated link</a>
                      </li>
                  </ul>
              </div>
          </div>
      </div>
      
      <div class="panel-body">
          <div class="row">
              <div class="col-lg-12">
                  <div class="table-responsive member">
                    <div class="col-lg-2 down">
                        <%= raw generated_link_to(image_tag("http://res.cloudinary.com/sampulkreativ/image/upload#{loaded_rand("profile", current_admin.id)}/profiles/#{current_admin.id}", class:"admin-avatar"),"#view_profile",{:id=>"view_profiles",:role=>"button", :onclick => "view_profile(#{current_admin.id},'no')", :data => {:toggle=>"modal", :url=>"view_profiles"}})%>

                        <span class="user-circle-online btn btn-success btn-circle online"></span>
                        <span class="left clearfix"></span>
                        <center class="admin_name"><%= current_admin.name %></center>
                    </div>
                    <% admins.each do |admin| %>
                      <div class="col-lg-2 down">
                            <%= raw generated_link_to(image_tag("http://res.cloudinary.com/sampulkreativ/image/upload#{loaded_rand("profile", admin.id)}/profiles/#{admin.id}",class:"admin-avatar thumb",:overwrite => false),"#view_profile",{:id=>"view_profiles",:role=>"button", :onclick => "view_profile(#{admin.id},'yes')", :data => {:toggle=>"modal", :url=>"view_profiles"}})%>
                            <span class="user-circle-online btn btn-success btn-circle online"></span>
                            <span class="left clearfix"></span>
                            <center><%= admin.name %></center>
                      </div>
                    <% end %>
                  </div>
              </div>
          </div>
      </div>
  </div>