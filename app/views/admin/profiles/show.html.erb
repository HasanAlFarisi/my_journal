  <div id="main">
    <div class="cl">&nbsp;</div>
    <div id="content">
      <!-- Box -->
      <div class="box">
        <!-- Box Head -->
        <div class="box-head">
          <h2><%= "#{@admin_profile.name} #{@admin_profile.last_name}" %></h2>
        </div>
        <!-- End Box Head -->
          <!-- Form -->
          <div class="form">
            <div class="left-photo">
            <%= image_tag @admin_profile.avatar.url(:original),class:"img_profile floating" %> 
            </div>
            
            <p>
              <%= label_tag :name, "Name" %>
              <%= @admin_profile.name %>
            </p>  
            <p>
              <%= label_tag :name, "Last Name" %>
              <%= @admin_profile.last_name %>
            </p> 
            <p>
              <%= label_tag :name, "Facebook" %>
              <%= @admin_profile.facebook %>
            </p>
            <p>
              <%= label_tag :name, "E-mail" %>
              <%= @admin_profile.e_mail %>
            </p>
            <p>
              <%= label_tag :skill, "Skill" %>
              <%= @admin_profile.e_mail %>
            </p>
            <p>
              <%= label_tag :hobby, "Hobby" %>
              <%= @admin_profile.e_mail %>
            </p>
            <p>
              <%= label_tag :motto, "Motto" %>
              <%= @admin_profile.e_mail %>
            </p>
          </div>
          <!-- End Form -->
          
          <!-- Form Buttons -->
          <div class="buttons">
            <%#= link_to "Back", admin_profiles_path, class:"button-back" %>
            <%= link_to "Edit Profile", edit_admin_profile_path(@admin_profile), class:"button" %>
          </div>
          <!-- End Form Buttons -->
      </div>
      <!-- End Box -->
    </div>
    <div class="cl">&nbsp;</div>
  </div>

<!-- Sidebar -->
<div id="sidebar">
  <%= render :partial => "admin/shared/sidebar" %>
</div>
<!-- Sidebar -->
