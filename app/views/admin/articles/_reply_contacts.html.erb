<% reply_contacts.each do |comment| %>
    	<tbody class='hidden_<%="#{comment.id}"%>'>
		<tr>
		  	<td style="text-align:left"><%= comment.body %></td>
		  	<td  width="5px"><%= link_to 'Destroy', admin_message_destroy_reply_path(comment), method: :delete, remote: true, :class => "ico del", onclick:"hidden_delete(#{comment.id})" %>
		  	</td>
		 </tr>
	</tbody>
<% end %>